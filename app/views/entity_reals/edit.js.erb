document.title = "<%=t 'title.abstract'%> :: <%=t 'title.entity'%> :: <%=t 'title.edit'%>";
if($("#body").html().length <= 1) {
  $("#body").html("<%= escape_javascript(render :partial => 'entity_reals/index') %>");
}
$("#entity_real_errors").html("<%= escape_javascript(render :partial => 'entity_reals/form_errors') %>");
$("#entity_real_actions").html("<%= escape_javascript(render :partial => 'entity_reals/form_actions') %>");
showEntityRealActionForm(unescape($("#hide_entity_real_tag").val()), "edit");
$("#entity_real_choose").click(function() {
  entityRealChooseSurrogates(<%=@entity_real.id%>);
});
if (!compareHashKeys(getPageSessionData("entity_reals", "surrogates"),
    getPageSessionData("entity_reals", "base_surrogates")) ||
    getPageSessionData("entity_reals", "tag") != getPageSessionData("entity_reals", "base_tag")) {
  $('#save_entity_real').removeAttr('disabled');
}
