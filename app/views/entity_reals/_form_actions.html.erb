<td>
  <input name="cancel" type="button" value="<%=t 'entity.btn_cancel'%>"
    onclick="onCancelEntity();"/>
  <input name="reset" type="button" value="<%=t 'entity.btn_reset'%>"
    onclick="onResetEntity();"/>
</td>
<td style="width: 45px">
  <%= form_for(@entity_real, :remote => true) do |f| %>
  <%= f.submit t('entity.btn_save'), :id => "save_entity_real", :disabled => "true",
                        :onclick => "sendTagEntity();" %>
  <%= f.hidden_field :tag, :id => "hide_entity_real_tag", :value => @entity_real.tag %>
  <% end %>
</td>
