document.title = "<%=t 'title.abstract'%> :: <%=t 'title.waybill'%>";
$("#body").html("<%= escape_javascript(render :partial => 'waybills/index') %>");
if(location.hash != "#waybills") {
  cancelRequest = true;
  location.hash = "#waybills";
}
dataPage = null;
var canManageWaybill = "<%= can?(:manage, Storehouse) %>";
if(canManageWaybill == "true") {
  $("#waybills_add").removeAttr("disabled");
  $("#waybills_add_1").removeAttr("disabled");
} else {
  $("#waybills_add").attr("disabled", "disabled");
  $("#waybills_add_1").attr("disabled", "disabled");
}
