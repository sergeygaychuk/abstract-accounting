<%= raw(storehouse_jqgrid) %>
<table>
  <tr>
    <td align="right">
      <input type="button" id="add_release" value="<%=t 'storehouse.btn_new_release'%>"
             onclick="location.hash = '#storehouses/releases/new';" />
      <input type="button" id="add_waybill" value="<%=t 'storehouse.btn_new_waybill'%>"
             onclick="location.hash = '#waybills/new';" />
    </td>
  </tr>
  <tr>
    <td>
      <table id=storehouse_list></table>
      <div id=storehouse_pager></div>
    </td>
  </tr>
</table>

<script type="text/javascript">
  $("#storehouse_list").filterToolbar({searchOnEnter : false});
  $("#storehouse_list")[0].toggleToolbar();
</script>
