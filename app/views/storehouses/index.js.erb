document.title = "<%=t 'title.abstract'%> :: <%=t 'title.storehouse'%>";
$("#body").html("<%= escape_javascript(render :partial => 'storehouses/index') %>");
$("#storehouseDiv").html("<%= escape_javascript(render :partial => 'storehouses/view') %>");
if(location.hash != "#storehouses") {
  cancelRequest = true;
  location.hash = "#storehouses";
}
dataPage = null;
if("<%= can?(:manage, Storehouse) %>" == "true") {
  $("#add_release").removeAttr("disabled");
  $("#add_waybill").removeAttr("disabled");
  $("#add_release_1").removeAttr("disabled");
  $("#add_waybill_1").removeAttr("disabled");
} else {
  $("#add_release").attr("disabled", "disabled");
  $("#add_waybill").attr("disabled", "disabled");
  $("#add_release_1").attr("disabled", "disabled");
  $("#add_waybill_1").attr("disabled", "disabled");
}
