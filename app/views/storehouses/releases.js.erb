$("#body").html("<%= escape_javascript(render :partial => 'storehouses/index') %>");
$("#storehouseDiv").html("<%= escape_javascript(render :partial => 'storehouses/releases') %>");
document.title = "Abstract::Storehouse::Releases";
if(location.hash != "#storehouses/releases") {
  cancelRequest = true;
  location.hash = "#storehouses/releases";
}
if("<%= can?(:manage, Storehouse) %>" == "true") {
  $("#release_new").removeAttr("disabled");
  $("#releases_list").hideCol("status");
} else {
  $("#release_new").attr("disabled", "disabled");
  $("#releases_list").showCol("status");
}
