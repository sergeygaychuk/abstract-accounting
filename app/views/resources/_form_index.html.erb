<table>
  <tr align="right">
    <td>
      <%= button_to "new", new_asset_resources_path, :method => :get,
          :remote => true %>
    </td>
    <td width="70">
      <%= button_to "change", edit_asset_resource_path(0),
          :id => 'change_resource', :method => :get, :remote => true,
          :disabled => true %>
    </td>
  </tr>
  <tr align="left">
    <td colspan="2">
      monetary:
      <input id="resource_type" type="checkbox" disabled="true"/>
    </td>
  </tr>
  <tr align="left">
    <td colspan="2">
      tag:
      <input id="resource_tag" type="text" disabled="true" value="" size="83" />
    </td>
  </tr>
</table>
