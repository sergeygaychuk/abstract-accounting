  <% end %>

  <table>
    <tr id="actions" align="right">
      <%= render 'form_index' %>
    </tr>
    <tr>
      <td style="width: 724px">
        resource:
        <input type="button" id="quote_res_btn" value="click to choose"
               disabled="true" onclick=""/>
